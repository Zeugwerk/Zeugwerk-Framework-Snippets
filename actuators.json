{
    "${1}DigitalMonoPlusLsPlusUM": {
        "isFileTemplate": true,
        "body": [
            "PROGRAM ${TM_FILENAME_BASE}",
            "VAR",
            "  DateTime : ZAux.DateTimeUM;",
            "  Logger : ZAux.LoggerFile7FFUM(datetime:=DateTime, filePath:='C:\temp\\logfile.log', target:='');",
            "  ${1:ActuatorInstance} : ZEquipment.ActuatorDigitalMonoPlusLsPlusUM;",
            "  Step : ZCore.Step(begin:=0, end:=100);",
            "END_VAR",
            "",
            "",            
            "DateTime.Cyclic();",
            "Logger.Cyclic();",
            "${1}.Cyclic();",
            "",
            "CASE Step.Index OF",
            "  0:",
            "    ${1}.SetLogger(Logger);",
            "    ${1}.SetName(name:='${2:ActuatorName}', movePlusName:='${2:PlusName}', moveMinusName:='${3:MinusName}', plusName:='Is${2}', minusName:='Is${3}');",
            "    ${1}.SetSimulation(mode:=ZEquipment.ActuatorDigitalSimulation.Automatic);",
            "    ${1}.SetSimulationUncertainty(uncertainty:=10);",
            "    ${1}.SetDelay(plusStartDuration:=0, plusEndDuration:=0, minusStartDuration:=0, minusEndDuration:=0, stop:=0);",
            "    ${1}.SetSwitchDuration(plusDuration:=0, minusDuration:=0);",
            "    ${1}.SetMovementDuration(plusDuration:=3, minusDuration:=3);",
            "    ${1}.SetTimeout(plusDuration:=5, minusDuration:=0);",
            "",
            "    Step.SetNext(10);",
            "",
            "  10:",
            "    IF Step.OnEntry() THEN",
            "      ${1}.MovePlusAsync(0);",
            "    END_IF",
            "",
            "    IF ${1}.Done THEN",
            "      Step.SetNext(20);",
            "    END_IF",
            "",
            "  20:",
            "    ;",
            "END_CASE",
            "",
            "", 
            "END_PROGRAM"            
        ],
        "description": "ActuatorDigitalMonoPlusLsPlusUM Example"
    }
}
